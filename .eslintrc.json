{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["google", 
    "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "new-cap": "off",
        "require-jsdoc": "off"
    }
}
